{
  "name": "altavant-azure-blob",
  "version": "0.0.1",
  "description": "Extended functionality of @azure/storage-blob",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "nodemon --config nodemon.json",
    "build": "sh -c '[ -d dist ] && rm -R dist' && tsc --build tsconfig.prod.json",
    "push": "pnpm build && git add . && git commit -m 'build' && git push",
    "test": "jest",
    "updatemods": "pnpm up @alshaya/list-data-reader --latest"
  },
  "dependencies": {
    "@azure/core-paging": "^1.6.2",
    "@azure/storage-blob": "^12.24.0",
    "list-data-reader": "github:moxalibudbud/list-data-reader#main",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "tslib": "^2.6.3"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./blob": "./dist/blob.js",
    "./container": "./dist/container.js",
    "./enums": "./dist/enums.js",
    "./service-client": "./dist/service-client.js",
    "./blob-line-reader": "./dist/blob-line-reader.js"
  }
}
